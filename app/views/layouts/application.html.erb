<!DOCTYPE html>
<html>
  <head>
    <title>BrightPeople</title>
    <%= yield :head %>
    <%= stylesheet_link_tag 'application', :media => "all" %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div id="wrapper">
      <div id="header">
        <%= render partial: 'shared/header' %>
      </div>

      <%= yield %>

      <div id="footer">
        <%= render partial: 'shared/footer' %>
      </div>

    </div>
  </body>
</html>
